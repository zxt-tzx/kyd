{
  "name": "kyd-agent",
  "main": "src/server.ts",
  "compatibility_date": "2025-03-14",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "DevResearchAgent",
        "class_name": "DevResearchAgent"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["DevResearchAgent"]
    }
  ],
  "vars": {
    "ENVIRONMENT": "development",
    "DEBUG_MODE": "true"
  },
  "env": {
    "stg": {
      "name": "kyd-agent-stg",
      "durable_objects": {
        "bindings": [
          {
            "name": "DevResearchAgent",
            "class_name": "DevResearchAgent"
          }
        ]
      },
      "vars": {
        "ENVIRONMENT": "staging",
        "DEBUG_MODE": "true"
      }
    },
    "prod": {
      "name": "kyd-agent-prod",
      "durable_objects": {
        "bindings": [
          {
            "name": "DevResearchAgent",
            "class_name": "DevResearchAgent"
          }
        ]
      },
      "vars": {
        "ENVIRONMENT": "production",
        "DEBUG_MODE": "false"
      }
    }
  }
}
