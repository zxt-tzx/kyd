{
  "name": "@know-your-dev/core",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "sst shell vitest",
    "test:watch": "sst shell -- vitest watch",
    "typecheck": "tsc",
    "db": "sst shell drizzle-kit",
    "db:generate": "sst shell -- drizzle-kit generate",
    "db:migrate": "sst shell -- drizzle-kit migrate",
    "db:migrate:prod": "sst shell --stage prod -- drizzle-kit migrate",
    "db:migrate:uat": "sst shell --stage uat -- drizzle-kit migrate",
    "db:push": "sst shell -- drizzle-kit push"
  },
  "exports": {
    "./*": [
      "./src/*/index.ts",
      "./src/*.ts"
    ]
  },
  "dependencies": {
    "@openauthjs/openauth": "^0.3.6",
    "dedent": "^1.5.3",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.6.1",
    "gql.tada": "^1.8.10",
    "graphql": "^16.10.0",
    "octokit": "^4.1.2",
    "postgres": "^3.4.4",
    "resend": "^4.1.1",
    "tldts": "^6.1.68",
    "ulidx": "^2.4.1",
    "zod": "*"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "drizzle-kit": "^0.30.5",
    "vitest": "^3.0.2"
  }
}
